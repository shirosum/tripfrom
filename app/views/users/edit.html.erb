
    <body id="bodyeuser">
        <div class="container">
            <div class="row">
                <h2>ユーザー編集</h2>
                <%= form_with model: @user, local: true do |f| %>
                    <h4>ユーザー</h4>
                    <%= f.text_field :user_name %>
                    <%= f.file_field :avatar %>
                    <%= link_to 'back', user_path(@user), class: "btn btn-default" %>
                    <%= f.submit '更新', class: "btn btn-default" %>
                <% end %>
            </div>
        </div>
    </body>
